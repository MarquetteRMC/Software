<launch>
    
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find robot_config)/urdf/myrobot.urdf" />
    <param name="use_gui" value="true" />
    <rosparam file="$(find arm_control)/config/controllers.yaml", command="load">

    <include file="$(find odrive_ros)/launch/odrive.launch" />
    <include file="$(find roboclaw_node)/launch/roboclaw_nodes.launch" />

    <node name ="controller_spawner" pkg="controller_manager" type"spawner" respawn="false" 
        output="screen" ns="/arm_controller" args="arm_state joint_state_controller">

   <!-- <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />-->
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" 
        respawn="false" output="screen" />

    <!-- <node pkg="overseer" type="overseer_node.py" name="overseer_node"> -->
    <node pkg="current_sensing" type="30A_current_sensor.py" name="current_sensing" />
    <!-- <node pkg="current_sensing" type="VoltageInput.py" name="battery_draw" /> -->
    
</launch>
